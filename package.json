    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue": "^6.5.10",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.7.1",
    "@types/uuid": "^8.3.4",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.4.4",
    "babel-loader": "^8.2.5",
    "babel-preset-vue": "^2.0.2",
    "css-loader": "^5.2.7",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
